/ {
	mtr{
		compatible = "gpio-leds";  
		in1: in_1 {
			gpios = <&gpio0 2 GPIO_ACTIVE_HIGH>;
			label = "motor1_gpio";  
		};
		in2: in_2 {
			gpios = <&gpio0 4 GPIO_ACTIVE_HIGH>;
			label = "motor2_gpio";  
		};
		in3: in_3 {
			gpios = <&gpio0 18 GPIO_ACTIVE_HIGH>;
			label = "motor3_gpio";  
		};
		in4: in_4 {
			gpios = <&gpio0 19 GPIO_ACTIVE_HIGH>;
			label = "motor4_gpio";  
		};
	};
};
